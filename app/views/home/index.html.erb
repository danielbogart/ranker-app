  <div class="page-header">
    <h1>Latest items</h1>
  </div>
  
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Title</th>
        <th>Price</th>
        <th>Happiness</th>
        <th>H Index</th>
      </tr>
    </thead>
    <tbody>
      <% @items.each do |p| %>
      <tr>
        <td>
          <%= p.title %>
        </td>
        <td>
          <%= p.price %>
        </td> 
        <td>
          <%= p.happiness %>
        </td>
        <td>
          <%= p.happines_index %>
        </td>
      </tr>
      <% end %>
    </tbody>
  </table>